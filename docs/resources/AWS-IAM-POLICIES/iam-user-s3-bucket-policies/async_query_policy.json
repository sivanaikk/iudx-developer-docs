{
    "Version": "2012-10-17",
    "Statement": [{
   		 "Action": [
   			 "s3:ListBucket",
   			 "s3:GetBucketLocation",
   			 "s3:ListBucketMultipartUploads",
   			 "s3:ListBucketVersions",
   			 "s3:ListBucketMultipartUploads"
   		 ],
   		 "Effect": "Allow",
   		 "Resource": [
   			 "arn:aws:s3:::<rs-async-bucket-name>"
   		 ]
   	 },
   	 {
   		 "Action": [
   			 "s3:GetObject",
   			 "s3:PutObject",
   			 "s3:DeleteObject",
   			 "s3:AbortMultipartUpload",
   			 "s3:ListMultipartUploadParts"
   		 ],
   		 "Effect": "Allow",
   		 "Resource": [
   			 "arn:aws:s3:::<rs-async-bucket-name>/*"
   		 ]
   	 }
    ]
}

